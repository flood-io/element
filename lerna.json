{
	"lerna": "2.11.0",
	"npmClient": "yarn",
	"version": "2.0.5-canary.20",
	"useWorkspaces": true,
	"command": {
		"init": {
			"exact": true
		},
		"publish": {
			"conventionalCommits": true
		},
		"version": {
			"message": "chore(release): publish %s\n[skip ci]",
			"allowBranch": ["stable", "beta", "canary"]
		},
		"bootstrap": {
			"npmClient": "yarn",
			"useWorkspaces": true
		}
	},
	"ignoreChanges": ["assets", "coverage", "examples", "scripts", "**/*.md", "packages/docs/**"]
}
