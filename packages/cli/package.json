{
	"name": "@flood/element-cli",
	"version": "1.0.10-beta.34",
	"license": "Apache-2.0",
	"description": "The command line interface for Flood Element",
	"repository": "flood-io/element",
	"bin": {
		"element": "./dist/bin/element.js"
	},
	"private": false,
	"scripts": {
		"build": "tsc",
		"build:watch": "tsc --watch",
		"prepublish": "in-publish && yarn build || not-in-publish",
		"prepublishOnly": "echo 'prepublishOnly'",
		"prebuild": "rm -rf ./dist",
		"xbuild": "yarn ncc build -e @flood/element -e @flood/element-core -e @flood/element-compiler -e @flood/element-api -e yargs -e yeoman-generator -e yeoman-environment -e typescript -o dist ./bin/element.ts",
		"dev": "ts-node ./index.ts"
	},
	"dependencies": {
		"@flood/element": "^1.0.10-beta.34",
		"@flood/element-core": "^1.0.10-beta.34",
		"yargs": "^11.0.0",
		"yeoman-environment": "2.3.0",
		"yeoman-generator": "3.1.0",
		"ms": "^2.1.1",
		"ora": "^2.1.0",
		"update-check": "^1.5.2",
		"boxen": "^3.0",
		"chalk": "^2.4.1",
		"chokidar": "^2.0.4",
		"command-exists": "^1.2.7",
		"debug": "^3.1.0",
		"find-root": "^1.1.0",
		"winston": "^3.0.0"
	},
	"devDependencies": {
		"@types/boxen": "^3.0.1",
		"@types/chalk": "^2.2.0",
		"@types/chokidar": "^1.7.5",
		"@types/command-exists": "^1.2.0",
		"@types/find-root": "^1.1.1",
		"@types/ms": "^0.7.31",
		"@types/ora": "^1.3.4",
		"@types/semver": "^5.5.0",
		"@types/yargs": "^11.0.0",
		"@types/yeoman-generator": "^3.1.4",
		"@zeit/ncc": "^0.20.4",
		"in-publish": "^2.0.0"
	},
	"gitHead": "75ceb903881476ed98cbc911bfce5fe303a57d55",
	"publishConfig": {
		"access": "public"
	}
}
