{
	"name": "@flood/element",
	"version": "1.0.6-beta.2",
	"description": "Flood Element is a browser based load generation tool for easily testing modern web application performance",
	"main": "./index.js",
	"types": "./index.ts.d",
	"scripts": {
		"build": "./scripts/build.sh",
		"build:watch": "tsc --watch",
		"xxxprepublishOnly": "./scripts/build.sh",
		"test": "mocha --trace-warnings -r ts-node/register \"src/**/*.spec.ts\"",
		"doc": "yarn typedoc && yarn typedoc:puppeteer && ts-node --project ./tsconfig.doc.json ./scripts/docs/docs.ts",
		"doc:watch": "node --trace-warnings $(npm bin)/nodemon --ext ts --exec ts-node -- --project ./tsconfig.doc.json ./scripts/docs/docs.ts",
		"doc:once": "ts-node --project ./tsconfig.doc.json ./scripts/docs/docs.ts",
		"typedoc": "typedoc --tsconfig ./tsconfig.doc.json --json docs.json ./index.ts",
		"typedoc:puppeteer": "typedoc --json puppeteer-1.6-docs.json --includeDeclarations ../../node_modules/@types/puppeteer/index.d.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/flood-io/element.git"
	},
	"keywords": [
		"load",
		"testing",
		"puppeteer",
		"chrome",
		"browser",
		"testing",
		"testing"
	],
	"author": "Ivan Vanderbyl <ivan@flood.io>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/flood-io/element/issues"
	},
	"homepage": "https://github.com/flood-io/element#readme",
	"dependencies": {
		"@types/faker": "^4.1.3",
		"@types/mkdirp": "^0.5.2",
		"@types/node": "10.7.0",
		"@types/prettier": "^1.13.2",
		"@types/puppeteer": "^1.12.4",
		"assert": "^1.4.1",
		"async": "^2.6.0",
		"comment-parser": "^0.5.0",
		"csv": "^3.1.0",
		"cuid": "^1.3.8",
		"d3-array": "^1.2.0",
		"diff": "^3.3.0",
		"express": "^4.16.1",
		"faker": "^4.1.0",
		"fs-extra": "^4.0.2",
		"glob": "^7.1.2",
		"knuth-shuffle": "^1.0.8",
		"micromatch": "^4.0.2",
		"morgan": "^1.9.0",
		"prettier": "^1.10.2",
		"puppeteer": "^1.17.0",
		"raven": "^2.1.2",
		"recast": "^0.13.0",
		"sanitize-filename": "^1.6.1",
		"source-map": "^0.7.0",
		"strftime": "^0.10.0",
		"term-img": "^2.1.0",
		"tti-polyfill": "^0.2.2",
		"typescript": "^2.9.0",
		"vm2": "^3.5.2",
		"winston": "^3.0.0"
	},
	"devDependencies": {
		"@types/chai": "^4.0.10",
		"@types/chai-as-promised": "^7.1.0",
		"@types/cuid": "^1.3.0",
		"@types/d3-array": "^1.2.1",
		"@types/debug": "^0.0.30",
		"@types/doctrine": "0.0.3",
		"@types/express": "^4.16.0",
		"@types/micromatch": "^3.1.0",
		"@types/mocha": "^2.2.45",
		"@types/raven": "^2.1.3",
		"@types/sanitize-filename": "^1.1.28",
		"@types/sinon-chai": "^2.7.29",
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"lodash.upperfirst": "^4.3.1",
		"mocha": "^4.0.1",
		"run-script-os": "^1.0.3",
		"sinon": "^4.1.3",
		"sinon-chai": "^2.14.0",
		"tslint": "^5.5.0",
		"tslint-config-prettier": "^1.5.0",
		"tslint-plugin-prettier": "^1.2.0"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 100,
		"useTabs": true,
		"tabWidth": 2,
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"arrowParens": "avoid"
	}
}
